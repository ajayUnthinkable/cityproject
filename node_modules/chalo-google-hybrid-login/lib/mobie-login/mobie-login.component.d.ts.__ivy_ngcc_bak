import { OnInit, EventEmitter, SimpleChanges } from '@angular/core';
import { FormGroup, FormBuilder } from '@angular/forms';
import { GoogleLoginService } from '../chalo-google-hybrid-login.service';
import { GoogleLoginResponse } from '../models';
export declare class MobieLoginComponent implements OnInit {
    private fb;
    private loginService;
    onSignInResponse: EventEmitter<GoogleLoginResponse>;
    showGoogleSignIn: EventEmitter<any>;
    showsendOtpForm: boolean;
    sendOtpForm: FormGroup;
    verifyOtpForm: FormGroup;
    showVerifyOtpForm: boolean;
    number: any;
    constructor(fb: FormBuilder, loginService: GoogleLoginService<any>);
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    sendOtp(hideSendOtpForm: any): void;
    verifyOtp(): void;
    onOtpChange(value: any): void;
}
